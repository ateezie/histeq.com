{
  "name": "historic-equity/wordpress-theme",
  "description": "WordPress theme for Historic Equity Inc. with Timber and FlyntWP",
  "type": "wordpress-theme",
  "require": {
    "timber/timber": "^2.0",
    "twig/twig": "^3.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.0",
    "phpcompatibility/php-compatibility": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "HistoricEquity\\": "lib/"
    }
  },
  "config": {
    "vendor-dir": "vendor"
  }
}